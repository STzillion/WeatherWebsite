{
        Console.WriteLine ("Put in seven numbers");
        
        // State how many numbers are being put as input.
        
        string word;
        word = Console.ReadLine();
        
        // Getting input as text first.
        
        int num;
        num = Convert.ToInt32(word);
        
        //Switching the input from string to integer.
        
        Console.WriteLine("Input = {0}", word);
        
        Console.WriteLine("Put in another seven numbers");
        
        //promt asking for another set of numbers.
        
        if(Console.ReadLine() == word)
        {
            Console.WriteLine("Input Matches");
            
            // matching first input with next set of input
        }
        else
        {
            Console.WriteLine("Input does not Match");
        }
    }
