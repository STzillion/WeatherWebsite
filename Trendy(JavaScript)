//Still working on this.. This is stuff with the API. The long string of numbers and words is the API key, don't really know how to move on from here.
var myHeaders = new Headers();
myHeaders.append("x-apisports-key", "615b0b4bfe5176acb9eaee55261d8850");
myHeaders.append("x-apisports-host", "v3.football.api-sports.io");

var requestOptions = {
  method: 'GET',
  headers: myHeaders,
  redirect: 'follow'
};
// This link(url) is supposed to be the website where the API returns the information from when requests are made.
fetch("https://v3.football.api-sports.io/{endpoint}", requestOptions)
  .then(response => response.text())
  .then(result => console.log(result))
  .catch(error => console.log('error', error));
